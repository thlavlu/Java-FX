<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="comboboxlistdemo.MainController">
   <children>
      <ComboBox fx:id="combobox" layoutX="175.0" layoutY="129.0" onAction="#showitemonlabel" prefWidth="150.0" promptText="Select an item" />
      <Label fx:id="result" layoutX="236.0" layoutY="95.0" text="Label" />
      <Button fx:id="btnadditem" layoutX="175.0" layoutY="174.0" mnemonicParsing="false" onAction="#additemaction" text="add item" />
      <ListView fx:id="listview" layoutX="342.0" layoutY="129.0" prefHeight="78.0" prefWidth="94.0" />
      <Button fx:id="btnshowitem" layoutX="342.0" layoutY="221.0" mnemonicParsing="false" onAction="#showitemaction" text="show item" />
   </children>
</AnchorPane>
